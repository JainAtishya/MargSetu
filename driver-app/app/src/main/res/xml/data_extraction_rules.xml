<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include shared preferences but exclude sensitive data -->
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="MargSetuDriverPrefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="."/>
        <exclude domain="sharedpref" path="MargSetuDriverPrefs.xml"/>
    </device-transfer>
</data-extraction-rules>